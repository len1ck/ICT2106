@{
    List<ActionModel> properties = ViewBag.uniquePropertyList;
    var category = ViewData["CATEGORY"] as string;
}

<h1>Quick Select Page Says Hi</h1>


<table class="table">
    <tr>
        <th>Category</th>
        <th>Action Properties</th>
        <th>Remarks</th>
    </tr>
    @foreach (var property in properties)
    {
        <tr>
            <td>@category</td>
            <td>
                <table style="border: none">
                    @for (int i = 0; i < property.ACTIONPROPERTYLIST.Count; i++)
                    {
                        <tr>
                            <td>@property.ACTIONPROPERTYNAME[i]</td>
                            <td>@property.ACTIONPROPERTYLIST[i]</td>
                        </tr>
                    }
                </table>
            </td>
            <td>
                <button onclick="location.href = '@Url.Action("ActionCategory","Action", property)'" class="btn btn-success">Select</button>
            </td>
        </tr>
    }
</table>